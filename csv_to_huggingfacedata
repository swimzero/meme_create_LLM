        "#데이터 경로 및 설정 불러오기\n",
        "datasetName = \"meme_dataset_ex.csv\"\n",
        "jsonFileName = \"meme_dataset_ex.jsonl\""
      ],
      "metadata": {
        "id": "bIo6KBdjo23l"
